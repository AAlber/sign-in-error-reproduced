  "private": true,
  "scripts": {
    "analyze": "ANALYZE=true doppler run -- next build",
    "build-local": "doppler run -- next build",
    "build": "next build",
    "check-types": "tsc --noEmit --pretty",
    "dev": "doppler run -- next dev",
    "dev:turbo": "NEXT_TURBOPACK_TRACING=1 doppler run -- next dev --turbo",
    "lint": "next lint",
    "prisma:migrate": "doppler run -- DATABASE_URL=\"$MIGRATE_DATABASE_URL\" prisma migrate deploy ",
    "prisma:push": "doppler run -- npx prisma db push",
    "prisma:studio": "doppler run -- npx prisma studio",
    "prisma": "doppler run -- npx prisma",
    "start": "npx prisma generate; next start",
    "stripe:listen": "stripe listen --forward-to localhost:3000/api/stripe/webhook --latest",
    "test-seeding": "doppler run -- npx prisma db seed",
    "test": "npm run cypress:run",
    "test:ci": "jest --ci --collectCoverage",
    "translate": "node translationFunc.js",
    "coverage": "doppler run -c github -- jest --collectCoverage --",
    "cypress:run": "cypress run",
    "cypress:open": "cypress open",
    "prepare": "husky install"
  },
  "prisma": {
    "seed": "doppler run -- ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
